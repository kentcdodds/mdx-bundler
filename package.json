{
  "name": "mdx-bundler",
  "version": "0.0.0-semantically-released",
  "description": "Give me MDX/TSX strings and I&#39;ll give you back a string of JS you can eval.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "keywords": [],
  "author": "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com)",
  "license": "MIT",
  "engines": {
    "node": ">=10",
    "npm": ">=6"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kentcdodds/mdx-bundler"
  },
  "bugs": {
    "url": "https://github.com/kentcdodds/mdx-bundler/issues"
  },
  "homepage": "https://github.com/kentcdodds/mdx-bundler#readme",
  "files": [
    "dist",
    "client.js",
    "client.d.ts"
  ],
  "scripts": {
    "build": "kcd-scripts build",
    "lint": "kcd-scripts lint",
    "setup": "npm install && npm run validate -s",
    "test": "kcd-scripts test",
    "test:update": "npm test -- --updateSnapshot --coverage",
    "typecheck": "kcd-scripts typecheck",
    "validate": "kcd-scripts validate"
  },
  "dependencies": {
    "@babel/preset-env": "^7.12.16",
    "@babel/preset-react": "^7.12.13",
    "@babel/preset-typescript": "^7.12.16",
    "@babel/runtime": "^7.12.13",
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "@rollup/plugin-babel": "^5.2.3",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "gray-matter": "^4.0.2",
    "rollup": "^2.39.0",
    "rollup-plugin-terser": "^7.0.2"
  },
  "devDependencies": {
    "@testing-library/react": "^11.2.5",
    "@types/jest": "^26.0.20",
    "@types/react": "^17.0.2",
    "@types/react-dom": "^17.0.1",
    "builtin-modules": "^3.2.0",
    "dotenv-cli": "^4.0.0",
    "kcd-scripts": "^7.6.0",
    "left-pad": "^1.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "typescript": "^4.1.5"
  },
  "eslintConfig": {
    "extends": "./node_modules/kcd-scripts/eslint.js",
    "rules": {
      "@typescript-eslint/no-unsafe-assignment": "off"
    }
  },
  "eslintIgnore": [
    "node_modules",
    "coverage",
    "dist",
    "*.d.ts"
  ]
}
